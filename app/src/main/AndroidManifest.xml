<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zoopark.demo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.zoopark.demo.empty.EmptyViewActivity"/>
        <activity android:name="com.zoopark.demo.animation.activity.TranslationAnimActivity" />
        <activity android:name="com.zoopark.demo.animation.activity.ScaleAnimActivity" />
        <activity android:name="com.zoopark.demo.animation.activity.FlipAnimActivity" />
        <activity android:name="com.zoopark.demo.animation.activity.HomeAnimActivity" />
        <activity android:name="com.zoopark.demo.complex.ComplexPageActivity" />
        <activity android:name="com.zoopark.demo.animation.activity.FadeAnimActivity" />
        <activity android:name="com.zoopark.demo.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>